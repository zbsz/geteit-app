<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LazyImageView">
        <attr name="thumbnail" format="boolean" />
        <attr name="fadeIn" format="boolean" />
        <attr name="loadAnimation" format="boolean" />
        <attr name="loadingDrawable" format="reference" />
        <attr name="errorDrawable" format="reference" />
    </declare-styleable>


    <declare-styleable name="RoboView">
        <attr name="fontFamily" format="enum" >
            <enum name="regular" value="0" />
            <enum name="light" value="1" />
            <enum name="thin" value="2" />
        </attr>
    </declare-styleable>

</resources>
